<div class="col-md-1"></div>
<div class="row">
  <div class="col-md-2"></div>
  <div class="col-lg-8 card card-header">
    <div class="card-body">
      <div class="row">
          <select class="btn-dark" onchange="calculateRoute(); routeInfo();" id="from">
            <% @campus.each do |from| %>
              <option value="<%= from.address %>"><%= from.name %></option>
            <% end %>
          </select>
          <select class="btn-dark" onchange="calculateRoute(); routeInfo();" id="to">
            <% @campus.each do |to| %>
              <option value="<%= to.address %>"
              <% if to.name == 'Vulkan' %>
                  selected=selected
                  <% end %>
              ><%= to.name %></option>
            <% end %>
          </select>
      </div>
      <div class="row">
        <div class="col-lg-8">
          <div id="map"></div>
        </div>
        <div id="route_info" class="col-lg-4">
            <%= render partial: 'route_info' %>
        </div>
      </div>
    </div>
  </div>
  <div class="col-md-2"></div>
</div>